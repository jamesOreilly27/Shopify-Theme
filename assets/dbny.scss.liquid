/***** Backgroun Colors *****/
body {
  color: "{{ settings.color_body }}";
  background-color: "{{ settings.bg_color_page }}"; }

.product-wrapper {
  background-color: "{{ settings.bg_color_wrapper }}"; }

.collection-wrapper {
  background-color: "{{ settings.bg_color_wrapper }}"; }

button {
  color: "{{ settings.color_btn }}";
  background-color: "{{ settings.bg_color_btn }}"; }
  button:active {
    background-color: "{{ settings.bg_color_btn_click }}"; }

h1 {
  color: "{{ settings.color_headings}}"; }

a {
  color: "{{ settings.color_links }}";
  text-decoration: none; }
  a:hover {
    color: "{{ settings.color_links_hover }}";
    transition: .3s; }

